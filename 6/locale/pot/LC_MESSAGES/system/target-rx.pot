msgid "qemu-system-rx -M gdbsim-r5f562n8 -bios <u-boot_image_file>"
msgstr ""

#: ../../../system/target-rx.rst:27
msgid ""
"Download ``kernel_image_file`` from https://osdn.net/users/ysato/pf/qemu/dl/"
"zImage"
msgstr ""

#: ../../../system/target-rx.rst:30
msgid ""
"Download ``device_tree_blob`` from https://osdn.net/users/ysato/pf/qemu/dl/"
"rx-virt.dtb"
msgstr ""

#: ../../../system/target-rx.rst:34
msgid "qemu-system-rx -M gdbsim-r5f562n8 \\"
msgstr ""

#: ../../../system/target-rx.rst:35
msgid ""
"-kernel <kernel_image_file> -dtb <device_tree_blob> \\ -append \"earlycon\""
msgstr ""
