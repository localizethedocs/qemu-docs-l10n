"{\n"
"    Error *err = NULL;\n"
"    bool ok = false;\n"
"    Visitor *v;\n"
"    UserDefOne *retval;\n"
"    q_obj_my_command_arg arg = {0};\n"
"\n"
"    v = qobject_input_visitor_new_qmp(QOBJECT(args));\n"
"    if (!visit_start_struct(v, NULL, NULL, 0, errp)) {\n"
"        goto out;\n"
"    }\n"
"    if (visit_type_q_obj_my_command_arg_members(v, &arg, errp)) {\n"
