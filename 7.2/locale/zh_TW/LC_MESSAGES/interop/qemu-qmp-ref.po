#: ../../../../qapi/virtio.json:-1
msgid ""
"-> { \"execute\": \"blockdev-open-tray\",\n"
"     \"arguments\": { \"id\": \"ide0-1-0\" } }\n"
"\n"
"<- { \"timestamp\": { \"seconds\": 1418751016,\n"
"                    \"microseconds\": 716996 },\n"
"     \"event\": \"DEVICE_TRAY_MOVED\",\n"
"     \"data\": { \"device\": \"ide1-cd0\",\n"
"               \"id\": \"ide0-1-0\",\n"
"               \"tray-open\": true } }\n"
"\n"
"<- { \"return\": {} }"
msgstr ""

