"\n"
"-> { \"execute\": \"blockdev-change-medium\",\n"
"     \"arguments\": { \"id\": \"floppyA\",\n"
"                    \"filename\": \"/srv/images/ro.img\",\n"
"                    \"format\": \"raw\",\n"
"                    \"read-only-mode\": \"read-only\" } }\n"
"\n"
"<- { \"return\": {} }"
msgstr ""

