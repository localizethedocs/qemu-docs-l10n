"       \"socket-id\": 0,\n"
"       \"drawer-id\": 0,\n"
"       \"book-id\": 0\n"
"     },\n"
"     \"cpu-state\": \"operating\",\n"
"     \"entitlement\": \"medium\",\n"
"     \"qom-path\": \"/machine/unattached/device[0]\",\n"
"     \"cpu-index\": 0,\n"
"     \"target\": \"s390x\"\n"
"   },\n"
"   {\n"
"     \"dedicated\": false,\n"
"     \"thread-id\": 537003,\n"
"     \"props\": {\n"
"       \"core-id\": 19,\n"
"       \"socket-id\": 1,\n"
"       \"drawer-id\": 0,\n"
"       \"book-id\": 2\n"
"     },\n"
"     \"cpu-state\": \"operating\",\n"
"     \"entitlement\": \"high\",\n"
"     \"qom-path\": \"/machine/peripheral-anon/device[0]\",\n"
"     \"cpu-index\": 19,\n"
"     \"target\": \"s390x\"\n"
"   },\n"
"   {\n"
"     \"dedicated\": false,\n"
"     \"thread-id\": 537004,\n"
"     \"props\": {\n"
