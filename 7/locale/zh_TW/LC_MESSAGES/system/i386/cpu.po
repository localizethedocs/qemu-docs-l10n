#: ../../../system/cpu-models-x86.rst.inc:404
msgid "Libvirt guest XML"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:406
msgid "Host passthrough::"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:408
msgid "<cpu mode='host-passthrough'/>"
msgstr ""

