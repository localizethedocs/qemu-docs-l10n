"    <feature name=\"vmx\" policy=\"disable\"/>\n"
"    ...\n"
"</cpu>"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:417
msgid "Host model::"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:419
msgid "<cpu mode='host-model'/>"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:421
msgid "Host model with feature customization::"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:423
msgid ""
"<cpu mode='host-model'>\n"
"    <feature name=\"vmx\" policy=\"disable\"/>\n"
"    ...\n"
"</cpu>"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:428
msgid "Named model::"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:430
msgid ""
"<cpu mode='custom'>\n"
"    <model name=\"Westmere\"/>\n"
"</cpu>"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:434
msgid "Named model with feature customization::"
msgstr ""

#: ../../../system/cpu-models-x86.rst.inc:436
msgid ""
"<cpu mode='custom'>\n"
"    <model name=\"Westmere\"/>\n"
"    <feature name=\"pcid\" policy=\"require\"/>\n"
"    ...\n"
"</cpu>"
msgstr ""
