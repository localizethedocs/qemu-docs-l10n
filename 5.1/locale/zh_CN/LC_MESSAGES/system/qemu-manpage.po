#: ../../../../qemu-options.hx:199 ../../../../qemu-options.hx:201
#: ../../../../qemu-options.hx:203 ../../../../qemu-options.hx:398
#: ../../../../qemu-options.hx:922 ../../../../qemu-options.hx:936
#: ../../../../qemu-options.hx:938 ../../../../qemu-options.hx:940
#: ../../../../qemu-options.hx:1238 ../../../../qemu-options.hx:1428
#: ../../../../qemu-options.hx:1430 ../../../../qemu-options.hx:1432
#: ../../../../qemu-options.hx:1549 ../../../../qemu-options.hx:1551
#: ../../../../qemu-options.hx:1553 ../../../../qemu-options.hx:3234
