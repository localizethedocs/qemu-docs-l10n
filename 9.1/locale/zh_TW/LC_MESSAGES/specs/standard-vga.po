msgid "``0500 - 0515``"
msgstr ""

#: ../../../specs/standard-vga.rst:83
msgid ""
"bochs dispi interface registers, mapped flat without index/data ports. Use "
"``(index << 1)`` as offset for (16bit) register access."
msgstr ""

#: ../../../specs/standard-vga.rst:85
msgid "``0600 - 0607``"
msgstr ""

#: ../../../specs/standard-vga.rst:86
msgid ""
"QEMU extended registers.  QEMU 2.2+ only. The pci revision is 2 (or greater) "
"when these registers are present. The registers are 32bit."
msgstr ""

#: ../../../specs/standard-vga.rst:89
msgid "``0600``"
msgstr ""

#: ../../../specs/standard-vga.rst:90
msgid "QEMU extended register region size, in bytes."
msgstr ""

#: ../../../specs/standard-vga.rst:91
msgid "``0604``"
msgstr ""

#: ../../../specs/standard-vga.rst:92
msgid ""
"framebuffer endianness register. - ``0xbebebebe`` indicates big endian. - "
"``0x1e1e1e1e`` indicates little endian."
msgstr ""
