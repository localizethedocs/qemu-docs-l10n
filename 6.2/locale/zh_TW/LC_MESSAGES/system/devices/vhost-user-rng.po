#: ../../../system/devices/vhost-user-rng.rst:22
msgid "The daemon should be started first:"
msgstr ""

#: ../../../system/devices/vhost-user-rng.rst:26
msgid "host# vhost-device-rng --socket-path=rng.sock -c 1 -m 512 -p 1000"
msgstr ""

#: ../../../system/devices/vhost-user-rng.rst:28
msgid ""
"The QEMU invocation needs to create a chardev socket the device can use to "
"communicate as well as share the guests memory over a memfd."
msgstr ""

#: ../../../system/devices/vhost-user-rng.rst:33
msgid ""
"host# qemu-"
"system                                                             \\\n"
"    -chardev socket,path=$(PATH)/rng.sock,"
"id=rng0                             \\\n"
"    -device vhost-user-rng-pci,"
"chardev=rng0                                   \\\n"
"    -m "
"4096                                                                   \\\n"
"    -object memory-backend-file,id=mem,size=4G,mem-path=/dev/shm,"
"share=on     \\\n"
"    -numa node,"
"memdev=mem                                                     \\\n"
"    ..."
msgstr ""
